{
  "name": "Table Talks",
  "image": "mcr.microsoft.com/devcontainers/python:1-3.13-bookworm",
  "containerEnv": {
    "UV_LINK_MODE": "copy"
  },
  "postCreateCommand": "pip install uv",
  "features": {
    "ghcr.io/devcontainers-extra/features/zsh-plugins:0": {
      "plugins": "npm git zsh-autosuggestions zsh-syntax-highlighting sudo",
      "omzPlugins": "https://github.com/zsh-users/zsh-autosuggestions https://github.com/zsh-users/zsh-syntax-highlighting.git",
      "username": "vscode"
    },
    "ghcr.io/stuartleeks/dev-container-features/shell-history:0": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "charliermarsh.ruff",
        "nsarrazin.pep723-uv-interpreter"
      ]
    }
  }
}
